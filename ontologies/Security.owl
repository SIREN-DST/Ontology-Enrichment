  <owl:Class rdf:ID="HoneyPot">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:InverseFunctionalProperty rdf:about="#protects"/>
        </owl:onProperty>
        <owl:someValuesFrom>
