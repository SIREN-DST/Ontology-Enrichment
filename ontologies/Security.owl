  <owl:Class rdf:ID="OpenPGP">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Standard"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="#DigitalSignatureData"/>
