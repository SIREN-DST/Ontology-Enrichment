        <standardControl_hasChild_StandardControl rdf:resource="http://securityontology.sba-research.org/securityontology.owl#A_15_3"/>
    </owl:NamedIndividual>
    


    <!-- http://securityontology.sba-research.org/securityontology.owl#A_15_1 -->

    <owl:NamedIndividual rdf:about="http://securityontology.sba-research.org/securityontology.owl#A_15_1">
        <rdf:type rdf:resource="http://securityontology.sba-research.org/securityontology.owl#ISO27001"/>
        <rdfs:label rdf:datatype="&xsd;string">A.15.1 Compliance with legal requirements</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string">To avoid breaches of any law, statutory, regulatory or contractual obligations, and of any security requirements.</rdfs:comment>
        <standardControl_hasParent_StandardControl rdf:resource="http://securityontology.sba-research.org/securityontology.owl#A_15"/>
        <standardControl_hasChild_StandardControl rdf:resource="http://securityontology.sba-research.org/securityontology.owl#A_15_1_1"/>
        <standardControl_hasChild_StandardControl rdf:resource="http://securityontology.sba-research.org/securityontology.owl#A_15_1_2"/>
        <standardControl_hasChild_StandardControl rdf:resource="http://securityontology.sba-research.org/securityontology.owl#A_15_1_3"/>
        <standardControl_hasChild_StandardControl rdf:resource="http://securityontology.sba-research.org/securityontology.owl#A_15_1_4"/>
        <standardControl_hasChild_StandardControl rdf:resource="http://securityontology.sba-research.org/securityontology.owl#A_15_1_5"/>
        <standardControl_hasChild_StandardControl rdf:resource="http://securityontology.sba-research.org/securityontology.owl#A_15_1_6"/>
    </owl:NamedIndividual>
    


    <!-- http://securityontology.sba-research.org/securityontology.owl#A_15_1_1 -->

    <owl:NamedIndividual rdf:about="http://securityontology.sba-research.org/securityontology.owl#A_15_1_1">
        <rdf:type rdf:resource="http://securityontology.sba-research.org/securityontology.owl#ISO27001"/>
        <rdfs:label rdf:datatype="&xsd;string">A.15.1.1 Identification of applicable legislation</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string">All relevant statutory, regulatory, and contractual requirements and the organization’s approach to meet these requirements should be explicitly defined, documented, and kept up to date for each information system and the organization.</rdfs:comment>
        <standardControl_hasParent_StandardControl rdf:resource="http://securityontology.sba-research.org/securityontology.owl#A_15_1"/>
        <standardControl_correspondsTo_control rdf:resource="http://securityontology.sba-research.org/securityontology.owl#ComplianceControl"/>
    </owl:NamedIndividual>
    


    <!-- http://securityontology.sba-research.org/securityontology.owl#A_15_1_2 -->

    <owl:NamedIndividual rdf:about="http://securityontology.sba-research.org/securityontology.owl#A_15_1_2">
        <rdf:type rdf:resource="http://securityontology.sba-research.org/securityontology.owl#ISO27001"/>
        <rdfs:label rdf:datatype="&xsd;string">A.15.1.2 Intellectual property rights (IPR)</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string">Appropriate procedures should be implemented to ensure compliance with legislative, regulatory, and contractual requirements on the use of material in respect of which there may be intellectual property rights and on the use of proprietary software products.</rdfs:comment>
        <standardControl_hasParent_StandardControl rdf:resource="http://securityontology.sba-research.org/securityontology.owl#A_15_1"/>
        <standardControl_correspondsTo_control rdf:resource="http://securityontology.sba-research.org/securityontology.owl#ComplianceControl"/>
    </owl:NamedIndividual>
    


    <!-- http://securityontology.sba-research.org/securityontology.owl#A_15_1_3 -->

    <owl:NamedIndividual rdf:about="http://securityontology.sba-research.org/securityontology.owl#A_15_1_3">
        <rdf:type rdf:resource="http://securityontology.sba-research.org/securityontology.owl#ISO27001"/>
        <rdfs:label rdf:datatype="&xsd;string">A.15.1.3 Protection of organizational records</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string">Important records should be protected from loss, destruction, and falsification, in accordance with statutory, regulatory, contractual, and business requirements.</rdfs:comment>
        <standardControl_hasParent_StandardControl rdf:resource="http://securityontology.sba-research.org/securityontology.owl#A_15_1"/>
        <standardControl_correspondsTo_control rdf:resource="http://securityontology.sba-research.org/securityontology.owl#OrganizationalRecordsProtectionControl"/>
    </owl:NamedIndividual>
    


    <!-- http://securityontology.sba-research.org/securityontology.owl#A_15_1_4 -->

    <owl:NamedIndividual rdf:about="http://securityontology.sba-research.org/securityontology.owl#A_15_1_4">
        <rdf:type rdf:resource="http://securityontology.sba-research.org/securityontology.owl#ISO27001"/>
        <rdfs:label rdf:datatype="&xsd;string">A.15.1.4 Data protection and privacy of personal information</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string">Data protection and privacy should be ensured as required in relevant legislation, regulations, and, if applicable, contractual clauses.</rdfs:comment>
        <standardControl_hasParent_StandardControl rdf:resource="http://securityontology.sba-research.org/securityontology.owl#A_15_1"/>
        <standardControl_correspondsTo_control rdf:resource="http://securityontology.sba-research.org/securityontology.owl#DataSecurityHandbookControl"/>
    </owl:NamedIndividual>
    


    <!-- http://securityontology.sba-research.org/securityontology.owl#A_15_1_5 -->

    <owl:NamedIndividual rdf:about="http://securityontology.sba-research.org/securityontology.owl#A_15_1_5">
        <rdf:type rdf:resource="http://securityontology.sba-research.org/securityontology.owl#ISO27001"/>
        <rdfs:label rdf:datatype="&xsd;string">A.15.1.5 Prevention of misuse of information processing facilities</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string">Users should be deterred from using information processing facilities for unauthorized purposes.</rdfs:comment>
        <standardControl_hasParent_StandardControl rdf:resource="http://securityontology.sba-research.org/securityontology.owl#A_15_1"/>
        <standardControl_correspondsTo_control rdf:resource="http://securityontology.sba-research.org/securityontology.owl#UnauthorizedUsePreventionControl"/>
    </owl:NamedIndividual>
    


    <!-- http://securityontology.sba-research.org/securityontology.owl#A_15_1_6 -->

    <owl:NamedIndividual rdf:about="http://securityontology.sba-research.org/securityontology.owl#A_15_1_6">
        <rdf:type rdf:resource="http://securityontology.sba-research.org/securityontology.owl#ISO27001"/>
        <rdfs:label rdf:datatype="&xsd;string">A.15.1.6 Regulation of cryptographic controls</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string">Cryptographic controls should be used in compliance with all relevant agreements, laws, and regulations.</rdfs:comment>
        <standardControl_hasParent_StandardControl rdf:resource="http://securityontology.sba-research.org/securityontology.owl#A_15_1"/>
        <standardControl_correspondsTo_control rdf:resource="http://securityontology.sba-research.org/securityontology.owl#ComplianceControl"/>
    </owl:NamedIndividual>
    


    <!-- http://securityontology.sba-research.org/securityontology.owl#A_15_2 -->

    <owl:NamedIndividual rdf:about="http://securityontology.sba-research.org/securityontology.owl#A_15_2">
        <rdf:type rdf:resource="http://securityontology.sba-research.org/securityontology.owl#ISO27001"/>
        <rdfs:label rdf:datatype="&xsd;string">A.15.2 Compliance with security policies and standards and technical compliance</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string">To ensure compliance of systems with organizational security policies and standards.</rdfs:comment>
        <standardControl_hasParent_StandardControl rdf:resource="http://securityontology.sba-research.org/securityontology.owl#A_15"/>
        <standardControl_hasChild_StandardControl rdf:resource="http://securityontology.sba-research.org/securityontology.owl#A_15_2_1"/>
        <standardControl_hasChild_StandardControl rdf:resource="http://securityontology.sba-research.org/securityontology.owl#A_15_2_2"/>
    </owl:NamedIndividual>
    


    <!-- http://securityontology.sba-research.org/securityontology.owl#A_15_2_1 -->

    <owl:NamedIndividual rdf:about="http://securityontology.sba-research.org/securityontology.owl#A_15_2_1">
        <rdf:type rdf:resource="http://securityontology.sba-research.org/securityontology.owl#ISO27001"/>
        <rdfs:label rdf:datatype="&xsd;string">A.15.2.1 Compliance with security policies and standards</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string">Managers should ensure that all security procedures within their area of responsibility are carried out correctly to achieve compliance with security policies and standards.</rdfs:comment>
        <standardControl_hasParent_StandardControl rdf:resource="http://securityontology.sba-research.org/securityontology.owl#A_15_2"/>
        <standardControl_correspondsTo_control rdf:resource="http://securityontology.sba-research.org/securityontology.owl#ComplianceControl"/>
    </owl:NamedIndividual>
    


    <!-- http://securityontology.sba-research.org/securityontology.owl#A_15_2_2 -->

    <owl:NamedIndividual rdf:about="http://securityontology.sba-research.org/securityontology.owl#A_15_2_2">
        <rdf:type rdf:resource="http://securityontology.sba-research.org/securityontology.owl#ISO27001"/>
        <rdfs:label rdf:datatype="&xsd;string">A.15.2.2 Technical compliance checking</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string">Information systems should be regularly checked for compliance with security implementation standards.</rdfs:comment>
        <standardControl_hasParent_StandardControl rdf:resource="http://securityontology.sba-research.org/securityontology.owl#A_15_2"/>
        <standardControl_correspondsTo_control rdf:resource="http://securityontology.sba-research.org/securityontology.owl#ComplianceControl"/>
    </owl:NamedIndividual>
    


    <!-- http://securityontology.sba-research.org/securityontology.owl#A_15_3 -->

    <owl:NamedIndividual rdf:about="http://securityontology.sba-research.org/securityontology.owl#A_15_3">
        <rdf:type rdf:resource="http://securityontology.sba-research.org/securityontology.owl#ISO27001"/>
        <rdfs:label rdf:datatype="&xsd;string">A.15.3 Information systems audit considerations</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string">To maximize the effectiveness of and to minimize interference to/from the information systems audit process.</rdfs:comment>
        <standardControl_hasParent_StandardControl rdf:resource="http://securityontology.sba-research.org/securityontology.owl#A_15"/>
        <standardControl_hasChild_StandardControl rdf:resource="http://securityontology.sba-research.org/securityontology.owl#A_15_3_1"/>
        <standardControl_hasChild_StandardControl rdf:resource="http://securityontology.sba-research.org/securityontology.owl#A_15_3_2"/>
    </owl:NamedIndividual>
    


    <!-- http://securityontology.sba-research.org/securityontology.owl#A_15_3_1 -->

    <owl:NamedIndividual rdf:about="http://securityontology.sba-research.org/securityontology.owl#A_15_3_1">
        <rdf:type rdf:resource="http://securityontology.sba-research.org/securityontology.owl#ISO27001"/>
        <rdfs:label rdf:datatype="&xsd;string">A.15.3.1 Information systems audit controls</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string">Audit requirements and activities involving checks on operational systems should be carefully planned and agreed to minimize the risk of disruptions to business processes.</rdfs:comment>
        <standardControl_hasParent_StandardControl rdf:resource="http://securityontology.sba-research.org/securityontology.owl#A_15_3"/>
        <standardControl_correspondsTo_control rdf:resource="http://securityontology.sba-research.org/securityontology.owl#AuditLoggingAndMonitoringControl"/>
    </owl:NamedIndividual>
    


    <!-- http://securityontology.sba-research.org/securityontology.owl#A_15_3_2 -->

    <owl:NamedIndividual rdf:about="http://securityontology.sba-research.org/securityontology.owl#A_15_3_2">
        <rdf:type rdf:resource="http://securityontology.sba-research.org/securityontology.owl#ISO27001"/>
        <rdfs:label rdf:datatype="&xsd;string">A.15.3.2 Protection of information systems audit tools</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string">Access to information systems audit tools should be protected to prevent any possible misuse or compromise.</rdfs:comment>
        <standardControl_hasParent_StandardControl rdf:resource="http://securityontology.sba-research.org/securityontology.owl#A_15_3"/>
        <standardControl_correspondsTo_control rdf:resource="http://securityontology.sba-research.org/securityontology.owl#DataEncryptionSoftwareControl"/>
    </owl:NamedIndividual>
    


