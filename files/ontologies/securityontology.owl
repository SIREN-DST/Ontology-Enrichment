    <!-- http://securityontology.sba-research.org/securityontology.owl#PCInternetAndEmailRegulationPolicy -->

    <owl:Class rdf:about="http://securityontology.sba-research.org/securityontology.owl#PCInternetAndEmailRegulationPolicy">
        <rdfs:label xml:lang="en">PC, Internet and Email Regulation Policy</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://securityontology.sba-research.org/securityontology.owl#InternetRegulationPolicy"/>
        <rdfs:comment xml:lang="en">&lt;p style=&quot;margin-top: 0&quot;&gt;
      Following points are part of this Policy:
    &lt;/p&gt;
    &lt;p style=&quot;margin-top: 0&quot;&gt;
      
    &lt;/p&gt;
    &lt;ul&gt;
      &lt;li&gt;
        PC Use Regulation
      &lt;/li&gt;
      &lt;li&gt;
        Restriction of Internet and Email Use (e.g. security settings in web 
        browsers, no automatic opening of mail attachments, ..)
      &lt;/li&gt;
      &lt;li&gt;
        Selection of adequate passwords and handling with passwords (using 
        different passwords for different services etc)
      &lt;/li&gt;
      &lt;li&gt;
        E-Mail: displaying of attachment-ending, disabling of automatic 
        opening and so on
      &lt;/li&gt;
      &lt;li&gt;
        Activation of Antivirus Software, conductiong regular checks, .
      &lt;/li&gt;
    &lt;/ul&gt;
    &lt;p&gt;
      &amp;#160;
    &lt;/p&gt;</rdfs:comment>
    </owl:Class>
    


