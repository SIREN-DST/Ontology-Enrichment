        <standardControl_correspondsTo_control rdf:resource="http://securityontology.sba-research.org/securityontology.owl#AuditLoggingAndMonitoringControl"/>
    </owl:NamedIndividual>
    


    <!-- http://securityontology.sba-research.org/securityontology.owl#A_10_10_2 -->

    <owl:NamedIndividual rdf:about="http://securityontology.sba-research.org/securityontology.owl#A_10_10_2">
        <rdf:type rdf:resource="http://securityontology.sba-research.org/securityontology.owl#ISO27001"/>
        <rdfs:label rdf:datatype="&xsd;string">A.10.10.2 Monitoring system use</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string">Procedures for monitoring use of information processing facilities should be established and the results of the monitoring activities reviewed regularly.</rdfs:comment>
        <standardControl_hasParent_StandardControl rdf:resource="http://securityontology.sba-research.org/securityontology.owl#A_10_10"/>
        <standardControl_correspondsTo_control rdf:resource="http://securityontology.sba-research.org/securityontology.owl#AuditLoggingAndMonitoringControl"/>
        <standardControl_correspondsTo_control rdf:resource="http://securityontology.sba-research.org/securityontology.owl#DataBreachNotificationControl"/>
    </owl:NamedIndividual>
    


    <!-- http://securityontology.sba-research.org/securityontology.owl#A_10_10_3 -->

    <owl:NamedIndividual rdf:about="http://securityontology.sba-research.org/securityontology.owl#A_10_10_3">
        <rdf:type rdf:resource="http://securityontology.sba-research.org/securityontology.owl#ISO27001"/>
