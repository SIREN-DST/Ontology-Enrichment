        <standardControl_correspondsTo_Control rdf:resource="http://securityontology.sba-research.org/securityontology.owl#FirewallControl"/>
        <standardControl_correspondsTo_control rdf:resource="http://securityontology.sba-research.org/securityontology.owl#FirewallControl"/>
        <standardControl_correspondsTo_control rdf:resource="http://securityontology.sba-research.org/securityontology.owl#NetworkLoggingAndMonitoringControl"/>
        <standardControl_correspondsTo_control rdf:resource="http://securityontology.sba-research.org/securityontology.owl#PersonalFirewallControl"/>
        <standardControl_correspondsTo_control rdf:resource="http://securityontology.sba-research.org/securityontology.owl#PositionResponsibilityControl"/>
        <standardControl_correspondsTo_control rdf:resource="http://securityontology.sba-research.org/securityontology.owl#WLANControl"/>
    </owl:NamedIndividual>
    


    <!-- http://securityontology.sba-research.org/securityontology.owl#A_10_6_2 -->

    <owl:NamedIndividual rdf:about="http://securityontology.sba-research.org/securityontology.owl#A_10_6_2">
        <rdf:type rdf:resource="http://securityontology.sba-research.org/securityontology.owl#ISO27001"/>
        <rdfs:label rdf:datatype="&xsd;string">A.10.6.2 Security of network services</rdfs:label>
