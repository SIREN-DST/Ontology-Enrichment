    </owl:NamedIndividual>
    


    <!-- http://securityontology.sba-research.org/securityontology.owl#A_10_5_1 -->

    <owl:NamedIndividual rdf:about="http://securityontology.sba-research.org/securityontology.owl#A_10_5_1">
        <rdf:type rdf:resource="http://securityontology.sba-research.org/securityontology.owl#ISO27001"/>
        <rdfs:label rdf:datatype="&xsd;string">A.10.5.1 Information back-up</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string">Back-up copies of information and software shall be taken and tested regularly in accordance with the agreed backup policy.</rdfs:comment>
        <standardControl_correspondsTo_control rdf:resource="http://securityontology.sba-research.org/securityontology.owl#BackupMoibleITDevicesControl"/>
        <standardControl_correspondsTo_control rdf:resource="http://securityontology.sba-research.org/securityontology.owl#BackupStorageInHouseControl"/>
        <standardControl_correspondsTo_control rdf:resource="http://securityontology.sba-research.org/securityontology.owl#BackupStorageOutsideControl"/>
