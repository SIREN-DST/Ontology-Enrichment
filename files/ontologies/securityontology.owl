        <rdfs:comment rdf:datatype="&xsd;string">Logging facilities and log information should be protected against tampering and unauthorized access.</rdfs:comment>
        <standardControl_hasParent_StandardControl rdf:resource="http://securityontology.sba-research.org/securityontology.owl#A_10_10"/>
        <standardControl_correspondsTo_control rdf:resource="http://securityontology.sba-research.org/securityontology.owl#AccessControlRegulationControl"/>
    </owl:NamedIndividual>
    


    <!-- http://securityontology.sba-research.org/securityontology.owl#A_10_10_4 -->

    <owl:NamedIndividual rdf:about="http://securityontology.sba-research.org/securityontology.owl#A_10_10_4">
        <rdf:type rdf:resource="http://securityontology.sba-research.org/securityontology.owl#ISO27001"/>
        <rdfs:label rdf:datatype="&xsd;string">A.10.10.4 Administrator and operator logs</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string">System administrator and system operator activities should be logged.</rdfs:comment>
        <standardControl_hasParent_StandardControl rdf:resource="http://securityontology.sba-research.org/securityontology.owl#A_10_10"/>
        <standardControl_correspondsTo_control rdf:resource="http://securityontology.sba-research.org/securityontology.owl#AuditLoggingAndMonitoringControl"/>
    </owl:NamedIndividual>
    


    <!-- http://securityontology.sba-research.org/securityontology.owl#A_10_10_5 -->

    <owl:NamedIndividual rdf:about="http://securityontology.sba-research.org/securityontology.owl#A_10_10_5">
        <rdf:type rdf:resource="http://securityontology.sba-research.org/securityontology.owl#ISO27001"/>
        <rdfs:label rdf:datatype="&xsd;string">A.10.10.5 Fault logging</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string">Faults should be logged, analysed, and appropriate action taken.</rdfs:comment>
        <standardControl_hasParent_StandardControl rdf:resource="http://securityontology.sba-research.org/securityontology.owl#A_10_10"/>
        <standardControl_correspondsTo_control rdf:resource="http://securityontology.sba-research.org/securityontology.owl#AuditLoggingAndMonitoringControl"/>
    </owl:NamedIndividual>
    


    <!-- http://securityontology.sba-research.org/securityontology.owl#A_10_10_6 -->

    <owl:NamedIndividual rdf:about="http://securityontology.sba-research.org/securityontology.owl#A_10_10_6">
        <rdf:type rdf:resource="http://securityontology.sba-research.org/securityontology.owl#ISO27001"/>
        <rdfs:label rdf:datatype="&xsd;string">A.10.10.6 Clock synchronization</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string">The clocks of all relevant information processing systems within an organization or security domain should be synchronized with an agreed accurate time source.</rdfs:comment>
        <standardControl_hasParent_StandardControl rdf:resource="http://securityontology.sba-research.org/securityontology.owl#A_10_10"/>
        <standardControl_correspondsTo_control rdf:resource="http://securityontology.sba-research.org/securityontology.owl#ClockSynchronizationControl"/>
    </owl:NamedIndividual>
    


    <!-- http://securityontology.sba-research.org/securityontology.owl#A_10_1_1 -->

    <owl:NamedIndividual rdf:about="http://securityontology.sba-research.org/securityontology.owl#A_10_1_1">
        <rdf:type rdf:resource="http://securityontology.sba-research.org/securityontology.owl#ISO27001"/>
        <rdfs:label rdf:datatype="&xsd;string">A.10.1.1 Documented operating procedures</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string">Operating procedures should be documented, maintaned, and made available to all users who need them.</rdfs:comment>
        <standardControl_hasParent_StandardControl rdf:resource="http://securityontology.sba-research.org/securityontology.owl#A_10_1"/>
        <standardControl_correspondsTo_control rdf:resource="http://securityontology.sba-research.org/securityontology.owl#DocumentedOperationalProceduresControl"/>
    </owl:NamedIndividual>
    


    <!-- http://securityontology.sba-research.org/securityontology.owl#A_10_1_2 -->

    <owl:NamedIndividual rdf:about="http://securityontology.sba-research.org/securityontology.owl#A_10_1_2">
        <rdf:type rdf:resource="http://securityontology.sba-research.org/securityontology.owl#ISO27001"/>
        <rdfs:label rdf:datatype="&xsd;string">A.10.1.2 Change management</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string">Changes to information processing facilities and systems should be controlled.</rdfs:comment>
        <standardControl_hasParent_StandardControl rdf:resource="http://securityontology.sba-research.org/securityontology.owl#A_10_1"/>
        <standardControl_correspondsTo_control rdf:resource="http://securityontology.sba-research.org/securityontology.owl#SoftwareApplicationEvaluationControl"/>
    </owl:NamedIndividual>
    


    <!-- http://securityontology.sba-research.org/securityontology.owl#A_10_1_3 -->

    <owl:NamedIndividual rdf:about="http://securityontology.sba-research.org/securityontology.owl#A_10_1_3">
        <rdf:type rdf:resource="http://securityontology.sba-research.org/securityontology.owl#ISO27001"/>
        <rdfs:label rdf:datatype="&xsd;string">A.10.1.3 Segregation of duties</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string">Duties and areas of responsibility should be segregated to reduce opportunities for unauthorized or unintenational modification or misuse of the organization&apos;s assets.</rdfs:comment>
        <standardControl_hasParent_StandardControl rdf:resource="http://securityontology.sba-research.org/securityontology.owl#A_10_1"/>
        <standardControl_correspondsTo_control rdf:resource="http://securityontology.sba-research.org/securityontology.owl#PositionResponsibilityControl"/>
    </owl:NamedIndividual>
    


    <!-- http://securityontology.sba-research.org/securityontology.owl#A_10_1_4 -->

    <owl:NamedIndividual rdf:about="http://securityontology.sba-research.org/securityontology.owl#A_10_1_4">
        <rdf:type rdf:resource="http://securityontology.sba-research.org/securityontology.owl#ISO27001"/>
        <rdfs:label rdf:datatype="&xsd;string">A.10.1.4 Separation of development, test, and operational facilities</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string">Development, test, and operational facilities should be separated to reduce risks of unathorised access or changes to the operational system.</rdfs:comment>
        <standardControl_hasParent_StandardControl rdf:resource="http://securityontology.sba-research.org/securityontology.owl#A_10_1"/>
        <standardControl_correspondsTo_control rdf:resource="http://securityontology.sba-research.org/securityontology.owl#SoftwareApplicationEvaluationControl"/>
    </owl:NamedIndividual>
    


    <!-- http://securityontology.sba-research.org/securityontology.owl#A_10_2 -->

    <owl:NamedIndividual rdf:about="http://securityontology.sba-research.org/securityontology.owl#A_10_2">
        <rdf:type rdf:resource="http://securityontology.sba-research.org/securityontology.owl#ISO27001"/>
        <rdfs:label rdf:datatype="&xsd;string">A.10.2 Third party services delivery management</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string">To implement and maintain the appropriate level of information security and service delivery in line with third party service delivery agreements.</rdfs:comment>
        <standardControl_hasParent_StandardControl rdf:resource="http://securityontology.sba-research.org/securityontology.owl#A_10"/>
        <standardControl_hasChild_StandardControl rdf:resource="http://securityontology.sba-research.org/securityontology.owl#A_10_2_1"/>
        <standardControl_hasChild_StandardControl rdf:resource="http://securityontology.sba-research.org/securityontology.owl#A_10_2_2"/>
        <standardControl_hasChild_StandardControl rdf:resource="http://securityontology.sba-research.org/securityontology.owl#A_10_2_3"/>
    </owl:NamedIndividual>
    


    <!-- http://securityontology.sba-research.org/securityontology.owl#A_10_2_1 -->

    <owl:NamedIndividual rdf:about="http://securityontology.sba-research.org/securityontology.owl#A_10_2_1">
        <rdf:type rdf:resource="http://securityontology.sba-research.org/securityontology.owl#ISO27001"/>
        <rdfs:label rdf:datatype="&xsd;string">A.10.2.1 Service delivery</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string">It should be ensured that the security controls, service definitions and delivery levels included in the third party service delivery agreement are implemented, operated, and maintained by the third party.</rdfs:comment>
        <standardControl_hasParent_StandardControl rdf:resource="http://securityontology.sba-research.org/securityontology.owl#A_10_2"/>
        <standardControl_correspondsTo_control rdf:resource="http://securityontology.sba-research.org/securityontology.owl#OutsourcingControl"/>
        <standardControl_correspondsTo_control rdf:resource="http://securityontology.sba-research.org/securityontology.owl#ThirdPartyServiceManagementControl"/>
    </owl:NamedIndividual>
    


    <!-- http://securityontology.sba-research.org/securityontology.owl#A_10_2_2 -->

    <owl:NamedIndividual rdf:about="http://securityontology.sba-research.org/securityontology.owl#A_10_2_2">
        <rdf:type rdf:resource="http://securityontology.sba-research.org/securityontology.owl#ISO27001"/>
        <rdfs:label rdf:datatype="&xsd;string">A.10.2.2 Monitoring and review of third party services</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string">The services, reports and records provided by the third party provided by the third party should be regularly monitored and reviewed, and audits should be carried out regularly.</rdfs:comment>
        <standardControl_hasParent_StandardControl rdf:resource="http://securityontology.sba-research.org/securityontology.owl#A_10_2"/>
        <standardControl_correspondsTo_control rdf:resource="http://securityontology.sba-research.org/securityontology.owl#ThirdPartyServiceManagementControl"/>
    </owl:NamedIndividual>
    


    <!-- http://securityontology.sba-research.org/securityontology.owl#A_10_2_3 -->

    <owl:NamedIndividual rdf:about="http://securityontology.sba-research.org/securityontology.owl#A_10_2_3">
        <rdf:type rdf:resource="http://securityontology.sba-research.org/securityontology.owl#ISO27001"/>
        <rdfs:label rdf:datatype="&xsd;string">A.10.2.3 Managing changes to third party services</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string">Changes to the provision of services, including maintaining and improving existing information security policies, procedures and controls, should be managed, taking account of the criticality of business systems and processes involved and re-assessment of risks.</rdfs:comment>
        <standardControl_hasParent_StandardControl rdf:resource="http://securityontology.sba-research.org/securityontology.owl#A_10_2"/>
        <standardControl_correspondsTo_control rdf:resource="http://securityontology.sba-research.org/securityontology.owl#ThirdPartyServiceManagementControl"/>
    </owl:NamedIndividual>
    


    <!-- http://securityontology.sba-research.org/securityontology.owl#A_10_3 -->

    <owl:NamedIndividual rdf:about="http://securityontology.sba-research.org/securityontology.owl#A_10_3">
        <rdf:type rdf:resource="http://securityontology.sba-research.org/securityontology.owl#ISO27001"/>
        <rdfs:label rdf:datatype="&xsd;string">A.10.3 System planning and acceptance</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string">To minimize the risk of system failures.</rdfs:comment>
        <standardControl_hasParent_StandardControl rdf:resource="http://securityontology.sba-research.org/securityontology.owl#A_10"/>
        <standardControl_hasChild_StandardControl rdf:resource="http://securityontology.sba-research.org/securityontology.owl#A_10_3_1"/>
        <standardControl_hasChild_StandardControl rdf:resource="http://securityontology.sba-research.org/securityontology.owl#A_10_3_2"/>
    </owl:NamedIndividual>
    


    <!-- http://securityontology.sba-research.org/securityontology.owl#A_10_3_1 -->

    <owl:NamedIndividual rdf:about="http://securityontology.sba-research.org/securityontology.owl#A_10_3_1">
        <rdf:type rdf:resource="http://securityontology.sba-research.org/securityontology.owl#ISO27001"/>
        <rdfs:label rdf:datatype="&xsd;string">A.10.3.1 Capacity management</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string">The use of resources should be monitored, tuned, and projections made of future capacity requirements to ensure the required system perfomance.</rdfs:comment>
        <standardControl_hasParent_StandardControl rdf:resource="http://securityontology.sba-research.org/securityontology.owl#A_10_3"/>
        <standardControl_correspondsTo_control rdf:resource="http://securityontology.sba-research.org/securityontology.owl#SystemCapacityMonitoringControl"/>
    </owl:NamedIndividual>
    


    <!-- http://securityontology.sba-research.org/securityontology.owl#A_10_3_2 -->

    <owl:NamedIndividual rdf:about="http://securityontology.sba-research.org/securityontology.owl#A_10_3_2">
        <rdf:type rdf:resource="http://securityontology.sba-research.org/securityontology.owl#ISO27001"/>
        <rdfs:label rdf:datatype="&xsd;string">A.10.3.2 System acceptance</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string">Acceptance criteria of new information systems, upgrades, and new versions should be established and suitable tests of the system(s) carried out during development and prior to acceptance.</rdfs:comment>
