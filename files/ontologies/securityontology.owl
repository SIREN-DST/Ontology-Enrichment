        <owl:disjointWith rdf:resource="http://securityontology.sba-research.org/securityontology.owl#RecordingOfConfigurationDataPolicy"/>
        <owl:disjointWith rdf:resource="http://securityontology.sba-research.org/securityontology.owl#RemovableMediaPolicy"/>
        <owl:disjointWith rdf:resource="http://securityontology.sba-research.org/securityontology.owl#RestrictiveGrantingOfAccessRightsPolicy"/>
        <owl:disjointWith rdf:resource="http://securityontology.sba-research.org/securityontology.owl#SecurityRelevantPatchesAndUpdatesPolicy"/>
        <owl:disjointWith rdf:resource="http://securityontology.sba-research.org/securityontology.owl#SecurityTrainingPolicy"/>
        <owl:disjointWith rdf:resource="http://securityontology.sba-research.org/securityontology.owl#ServiceContractPolicy"/>
        <owl:disjointWith rdf:resource="http://securityontology.sba-research.org/securityontology.owl#SocialNetworkPolicy"/>
        <owl:disjointWith rdf:resource="http://securityontology.sba-research.org/securityontology.owl#TeleworkingPolicy"/>
        <owl:disjointWith rdf:resource="http://securityontology.sba-research.org/securityontology.owl#VirusInfectionEmergencyRegulationPolicy"/>
        <owl:disjointWith rdf:resource="http://securityontology.sba-research.org/securityontology.owl#WLANPolicy"/>
        <rdfs:comment xml:lang="en">&lt;p style=&quot;margin-top: 0&quot;&gt;
      Policy should contain:
    &lt;/p&gt;
    &lt;ul&gt;
      &lt;li&gt;
        Regulation that every connection between internet and the internal 
        network must be secured with a firewall
      &lt;/li&gt;
      &lt;li&gt;
        Decision if Internet Provider should be responsible for 
        firewall-services (Managed Security Contract) or not
      &lt;/li&gt;
      &lt;li&gt;
        Updates for firewalls - who is responsible, when do the update
      &lt;/li&gt;
      &lt;li&gt;
        Configuration and administration of firewall - must be possible 
        through a secure connection
      &lt;/li&gt;
      &lt;li&gt;
        Configuration information - where and how to store, keep up to date
      &lt;/li&gt;
      &lt;li&gt;
        Determination which connections to and from the internal network are 
        allowed and disallowed
      &lt;/li&gt;
      &lt;li&gt;
        Determination of Logging-Level and responsibilty for regular checking 
        of logging-files
      &lt;/li&gt;
      &lt;li&gt;
        Configuration of Personal Firewalls
      &lt;/li&gt;
    &lt;/ul&gt;
    &lt;p&gt;
      &amp;#160;
    &lt;/p&gt;
    &lt;p style=&quot;margin-top: 0&quot;&gt;
      
    &lt;/p&gt;
    &lt;ul&gt;
      
    &lt;/ul&gt;
    &lt;p&gt;
      &amp;#160;
    &lt;/p&gt;</rdfs:comment>
