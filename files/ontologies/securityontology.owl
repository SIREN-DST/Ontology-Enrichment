        <rdf:type rdf:resource="http://securityontology.sba-research.org/securityontology.owl#ISO27001"/>
        <rdfs:label rdf:datatype="&xsd;string">A.10.4 Protection against malicious and mobile code</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string">To protect the integrity of software and information.</rdfs:comment>
        <standardControl_hasParent_StandardControl rdf:resource="http://securityontology.sba-research.org/securityontology.owl#A_10"/>
        <standardControl_hasChild_StandardControl rdf:resource="http://securityontology.sba-research.org/securityontology.owl#A_10_4_1"/>
